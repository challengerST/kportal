<div class="outer-con">
    <table class="outer-tb">
        <thead>
        <tr>
            <td>仓库编号</td>
            <td>仓库名称</td>
            <td>仓库类型</td>
            <td>仓库地址</td>
            <td>仓库地图</td>
            <td>联系人</td>
            <td>联系方式</td>
            <td>操作</td>
        </tr>
        </thead>
        <tbody>

        </tbody>
    </table>
